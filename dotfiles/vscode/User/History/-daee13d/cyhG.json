{
  "name": "ourafront",
  "version": "1.37.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server.ts",
    "dev-clean": "yarn cache clean && ts-node --project tsconfig.server.json server.ts",
    "prebuild": "ts-node utils/generateRobotsTxt.ts",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" && next build && next-sitemap",
    "ci-build-check": "IMGIX_DOMAIN=ouraring.imgix.net yarn build",
    "start": "tsc -p tsconfig.server.json && node --max-old-space-size=8192 dist/server.js",
    "storybook": "start-storybook -s .",
    "build-storybook": "build-storybook -o ./components/sormus/_docs",
    "test": "jest --verbose --runInBand",
    "test:all": "tsc -p . && jest --verbose && yarn live-server-test-local",
    "tsc": "tsc",
    "test:coverage": "jest --coverage --runInBand",
    "test:integration": "jest --config tests/integration/jest.config.js",
    "test:integration:local": "TEST_LOCAL_SERVER=1 jest --config tests/integration/jest.config.js",
    "live-server-test-prod": "export HOST=https://ouraring.com; jest --config ./infra/lambda/after-allow-traffic-hook/tests/jest.config.js",
    "live-server-test-staging": "export HOST=https://ecom.ourastage.com;jest --config ./infra/lambda/after-allow-traffic-hook/tests/jest.config.js",
    "live-server-test-dev": "export HOST=https://ecom.ourasandbox.com; jest --config ./infra/lambda/after-allow-traffic-hook/tests/jest.config.js",
    "live-server-test-local": "export HOST=http://localhost:8000;jest --config ./infra/lambda/after-allow-traffic-hook/tests/jest.config.js",
    "cypress-open-e2e": "cypress open --config-file=cypress.json --config baseUrl=http://localhost:8000",
    "cypress-run-e2e": "cypress run --config-file=cypress.json --config baseUrl=http://localhost:8000",
    "cypress-open-e2e-ui": "cypress open --config-file=cypress.json --config baseUrl=http://localhost:8000,integrationFolder=tests/cypress/integration/e2e-ui",
    "cypress-run-e2e-ui": "cypress run --config-file=cypress.json --config baseUrl=http://localhost:8000,integrationFolder=tests/cypress/integration/e2e-ui",
    "cypress-open-staging": "cypress open --config-file=cypress.json --config baseUrl=https://ecom.ourastage.com",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "yarn run lint --fix",
    "bundle-report-generate": "webpack --profile --json > stats.json",
    "bundle-report-front": "webpack-bundle-analyzer --port 4200 .next/stats.json",
    "bundle-report-server": "webpack-bundle-analyzer --port 4200 .next/server/stats.json",
    "chromatic": "npx chromatic --project-token 8eabe25fea6c",
    "postinstall": "husky install",
    "extract-locales": "ts-node ./utils/lokalise.ts --write ./src/localization",
    "extract-icons": "ts-node ./figma.ts",
    "generate-graphql-types": "node ./bin/regenerateGraphQLTypescript.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint . --ext .js,.jsx,.ts,.tsx --fix"
    ]
  },
